meta {
  name: list
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/models/list
  body: none
  auth: none
}

headers {
  Accept: application/json
}

assert {
  res.status: eq 200
}

tests {
  const data = res.getBody();
  
  test("should be an array", function() {
    expect(data).to.be.a('array');
  });
  
  test("should contain expected attributes", function() {
    expect(data[0].model_name).to.be.a('string');
    expect(data[0].pricing.cents_per_input_token).to.be.a('number');
  });
  
}
